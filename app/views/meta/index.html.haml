- page_title = title(t('.title'))

#content.section--primary{ tabindex: "0" }
  .container
    %h1
      %span= page_title

%section
  .container
    %ul.list--block
      - @meta_routes.each do |route|
        %li
          .card
            .card__details
              %h2= link_to(t("meta.#{route[:translation].underscore}.title"), route[:url])
